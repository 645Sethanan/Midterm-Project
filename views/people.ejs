<%-include('header.ejs')%>
<div class="container">
    <div class="row mt-4">
        <!-- select menu -->
            <% Object.values(people).forEach(function(item){%>
                <div class="col-lg-4 col-md-6 mb-4">
                    <div class="card h-100 ">
                        <a href="#"><img class="card-img-top" src="./images/<%=item.image%>" alt="img" style=" width: 100%; height: 300px;"></a>
                        <div class="card-body">
                            <h4 class="card-title">
                            <h3><%=item.name%></h3>
                            </h4>
                            <h5>qualification <%=item.qualification%> </h5>
                        </div>    
                    </div> 
                </div>
            <%})%>
    </div>
</div>
