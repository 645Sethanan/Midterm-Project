<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Foodscience Website</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
</head>
<body>
    <%-include('navbar-admin.ejs')  %>
    <div class="container  d-flex justify-content-center ">
        <div style="width: 100%;">
            <form action="/delete/people" method="post">
                <label >ID</label><br>
                <input type="text" name="id"><br>
                <input type="submit" value="delete">
            </form>
            <% Object.values(people).forEach(function(item){%>
                <div class="col-lg-4 col-md-6 mb-4">
                    <div class="card h-100">
                        <a href="#"><img class="card-img-top" src="./images/<%=item.image%>" alt="img"></a>
                        <div class="card-body">
                            <h4 class="card-title">
                            <h5>ID <%=item._id%></h5>
                            <a href="#"><%=item.name%></a>
                            </h4>
                            <h5>price <%=item.qualification%> Bath</h5>
                        </div>    
                    </div> 
                </div>
            <%})%>
        </div>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
</body>
</html>